{
  "manifest_version": 2,
  "name": "browser-scripts",
  "description": "extensions to browsing",
  "version": "0.1",
  "background": {
    "scripts": [],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["src/github.content.js"]
    }
  ],
  "content_security_policy": "script-src 'self' https://unpkg.com; object-src 'self'",
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "contextMenus"
  ]
}
